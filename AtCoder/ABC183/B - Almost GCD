void solve()
{
  
	 int n;
	 cin>>n;
	 vi v(n);
	 for(int &i : v)
	 	cin>>i;
		 	
	int mx = -1;
	int ans=0;
	int cnt=0;
	for(int i=2;i<=1000;i++)
	{
		for(int j=0;j<n;j++)
			if(v[j]%i == 0) cnt++;
		if(cnt>mx) mx = cnt, ans = i;	
		cnt=0;
	}
	cout<<ans<<endl;
}	
		



void solve()
{


	int n,k;
	cin>>n>>k;
	int u=2*k-n-1;
	f(i,1,u+1) cout<<i<<" ";
	for(int i=k;i>u;i--) cout<<i<<" ";
	cout<<'\n';
	
}
int main()
{
	fastio
	int tt;
	cin>>tt;
	while(tt--)
	{
	
      solve();

		
	}
	
		
		
	
    return 0;
}
